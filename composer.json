{
    "name": "ipolitic/nawp",
    "homepage": "https://github.com/MarcFaussurier/NAWP",
    "version": "0.1",
    "authors": [
        {
            "name": "FAUSSURIER Marc",
            "email": "faussurier.marc@icloud.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/iPolitic/phpsocket.io"
        },
        {
            "type": "git",
            "url": "https://github.com/iPolitic/Workerman"
        },
        {
            "type": "git",
            "url": "https://github.com/iPolitic/Solex"
        }
    ],
    "require": {
        "php": ">= 7.2.0",
        "twig/twig": "^2.5",
        "workerman/workerman": "dev-master",
        "ipolitic/solex": "dev-master",
        "workerman/phpsocket.io": "dev-master",
        "symfony/dotenv": "^4.2@dev",
        "ext-json": "*",
        "ext-openssl": "*",
        "atlas/orm": "3.x-dev",
        "atlas/cli": "2.x-dev",
        "friendsofphp/php-cs-fixer": "^2.14@dev",
        "zendframework/zend-diactoros": "^2.0@dev",
        "symfony/cache": "^4.2@dev",
        "psr/container": "^1.0@dev",
        "psr/http-server-middleware": "^1.0@dev",
        "psr/http-server-handler": "^1.0@dev",
        "jasny/http-message": "dev-master"
    },
    "require-dev": {
      "phpunit/phpunit": "7"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "App\\": ["src/", "bundles/"] }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "scripts": {
        "post-update-cmd": [
            "php ./configs/postbuild.php"
        ]
    }
}